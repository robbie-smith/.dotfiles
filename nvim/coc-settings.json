{
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "black",
  "python.pythonPath": "~/neovim3/bin/python3",
  "python.analysis.typeCheckingMode": "off",
  "python.analysis.extraPaths": ["~/Dev/ais/src/*"],
  "python.formatting.blackArgs":["~/neovim3/lib/python3.9/site-packages/black/"],
  "workspace.ignoredFolders": ["~/Dev/ais/package/"],
  "jedi.workspace.symbols.ignoreFolders": [
    ".nox",
    ".tox",
    ".venv",
    "__pycache__",
    "venv",
    "~/Dev/ais/package/"
  ]
}
